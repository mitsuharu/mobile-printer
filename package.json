{
  "name": "mobpri",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-dev": "react-native run-android --appIdSuffix dev",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "echo \"build command only checks typescript syntax and built-in lint.\" && yarn typecheck",
    "typecheck": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:project": "npx react-native-clean-project --keep-node-modules --remove-iOS-build --remove-iOS-pods --remove-android-build --clean-android-project --keep-brew --keep-pods",
    "clean:jest": "jest --clearCache",
    "test": "cross-env TZ=Asia/Tokyo jest",
    "lint": "eslint ./src/ --ext .js,.jsx,.ts,.tsx --quiet",
    "pod": "cd ios; pod install; cd -;"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-native": "0.70.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.1.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.1.2",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.4",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-jest": "^29.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-optional-require": "^0.3.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.0",
    "jest-date-mock": "^1.0.8",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^14.0.1",
    "jest-mock-extended": "^3.0.1",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
